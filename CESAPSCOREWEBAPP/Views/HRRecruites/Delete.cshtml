
@model CESAPSCOREWEBAPP.Models.Appointment


@{
    IEnumerable<Appointment> applists = ViewData["applist"] as IEnumerable<Appointment>;
    IEnumerable<HRRecruite> hrPersons = ViewData["hRRecruite"] as IEnumerable<HRRecruite>;


}


@{
    ViewData["Title"] = "การนัดหมาย";
    ViewData["Tab"] = "<ol class='breadcrumb'>"
                   + "<li>"
                   + "<a href='../../Home/Index'>Home</a>"
                   + "</li>"
                   + "<li>"
                   + "<a href='/HRRecruites/Index'>Recruit Data</a>"
                   + "</li>"
                   + "<li class='active'>"
                   + "<a>" + @ViewData["Title"] + "</a>"
                   + "</li>"
                   + "</ol>";


    var status = 0;
}







@foreach (HRRecruite hrPerson in hrPersons)
{
    status = hrPerson.HRRecruiteStatusId;




    <div class="col-lg-12">
        <div class="tabs-container">
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#tab-1"> การนัดหมาย</a></li>
                <li class=""><a data-toggle="tab" href="#tab-2"> ผลการ Recruit</a></li>
                <li class=""><a data-toggle="tab" href="#tab-3"> ประวัติส่วนตัว</a></li>
                <li class=""><a data-toggle="tab" href="#tab-4">ประวัติการศึกษา</a></li>
                <li class=""><a data-toggle="tab" href="#tab-5">ประวัติการทำงาน</a></li>
            </ul>
            <div class="tab-content">
                <div id="tab-1" class="tab-pane active">
                    <div class="panel-body">
                        <strong>การนัดหมาย :  @hrPerson.TitleOfUsers.TitleOfUserName@hrPerson.HRRecruiteFName   @hrPerson.HRRecruiteLName</strong>
                        <div class="wrapper wrapper-content animated fadeInRight">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="ibox product-detail">
                                        <div class="ibox-content">
                                            <div class="row">
                                                <table class="table table-striped table-bordered table-hover dataTables-example" id="dt1">
                                                    <thead>
                                                        <tr>
                                                            <th>ครั้งที่</th>
                                                            <th>วันที่ติดต่อ</th>
                                                            <th>ปรเภทการนัด</th>
                                                            <th>สถานะ</th>
                                                            <th>สถานะนัด</th>
                                                            <th>วันเวลานัด</th>
                                                            <th>สถานะสัมภาษณ์</th>
                                                            <th>หมายเหตุ</th>

                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        @{
                                                            var date1 = "";
                                                            var date2 = "";
                                                            var j = 1;
                                                            foreach (Appointment applist in applists)
                                                            {

                                                                <tr>
                                                                    <td>@j</td>
                                                                    <td>
                                                                        @{
                                                                            try
                                                                            {
                                                                                DateTime UpdatedTime = (DateTime)@applist.AppTelDate;
                                                                                date1 = UpdatedTime.ToString("dd/MM/yyyy");
                                                                            }
                                                                            catch
                                                                            {
                                                                                date1 = "";
                                                                            }
                                                                        }

                                                                        @date1


                                                                    </td>
                                                                    <td>@applist.appTelTypes.AppTelTypeName</td>
                                                                    <td>@applist.appStatuses.AppStatusName</td>
                                                                    <td>@applist.appResults.AppResultName</td>
                                                                    <td>

                                                                        @{
                                                                            try
                                                                            {
                                                                                DateTime UpdatedTime = (DateTime)@applist.AppDate;
                                                                                date2 = UpdatedTime.ToString("dd/MM/yyyy HH:mm");
                                                                            }
                                                                            catch
                                                                            {
                                                                                date2 = "";
                                                                            }
                                                                        }

                                                                        @date2



                                                                    </td>
                                                                    <td>@applist.appSuccesses.AppSuccessName</td>
                                                                    <td>@applist.appEtcs.AppEtcName</td>




                                                                </tr>
                                                                j++;
                                                            }
                                                        }
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                        <div class="ibox-footer">
                                            <span class="pull-right">
                                                วันที่เพิ่มข้อมูล - <i class="fa fa-clock-o"></i> @hrPerson.HRRecruitDate.ToString("dd/MM/yyyy HH:mm:ss")
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="tab-2" class="tab-pane">
                    <div class="panel-body">
                        <strong>สถานะการ Recruit :  @hrPerson.TitleOfUsers.TitleOfUserName@hrPerson.HRRecruiteFName   @hrPerson.HRRecruiteLName</strong>
                        <div class="wrapper wrapper-content animated fadeInRight">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="ibox product-detail">
                                        <div class="ibox-content">

                                            <div class="row">

                                                <div class="col-md-9 col-md-offset-3">

                                                    <h2 class="font-bold m-b-xs">

                                                        สถานะการ Recruit : @hrPerson.HRRecruiteStatuses.HRRecruiteStatusName
                                                    </h2>
                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price">
                                                            วันที่เซ็นสัญญา :

                                                            @{
                                                                var DateSign = "";
                                                                try
                                                                {
                                                                    DateTime UpdatedTime = (DateTime)@hrPerson.SignDate;
                                                                    DateSign = UpdatedTime.ToString("dd/MM/yyyy");
                                                                }
                                                                catch
                                                                {
                                                                    DateSign = "";
                                                                }

                                                            }

                                                            @DateSign
                                                        </h2>
                                                    </div>
                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price">
                                                            เริ่มทำงาน :

                                                            @{
                                                                var startwordk = "";
                                                                try
                                                                {
                                                                    DateTime UpdatedTime = (DateTime)@hrPerson.StartWork;
                                                                    startwordk = UpdatedTime.ToString("dd/MM/yyyy");
                                                                }
                                                                catch
                                                                {
                                                                    startwordk = "";
                                                                }

                                                            }

                                                            @startwordk
                                                        </h2>
                                                    </div>
                                                    <hr>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="ibox-footer">
                                            <span class="pull-right">
                                                วันที่เพิ่มข้อมูล - <i class="fa fa-clock-o"></i> @hrPerson.HRRecruitDate.ToString("dd/MM/yyyy HH:mm:ss")
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="tab-3" class="tab-pane">
                    <div class="panel-body">
                        <strong>ประวัติส่วนตัว :  @hrPerson.TitleOfUsers.TitleOfUserName@hrPerson.HRRecruiteFName   @hrPerson.HRRecruiteLName</strong>
                        <div class="wrapper wrapper-content animated fadeInRight">
                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="ibox product-detail">
                                        <div class="ibox-content">

                                            <div class="row">
                                                <div class="col-md-9 col-md-offset-3">
                                                    <h2 class="font-bold m-b-xs">
                                                        @hrPerson.TitleOfUsers.TitleOfUserName @hrPerson.HRRecruiteFName   @hrPerson.HRRecruiteLName
                                                    </h2>


                                                    <small>ฝ่าย :  @hrPerson.Organizs.Departments.DepartmentName  แผนก : @hrPerson.Organizs.Department1s.Department1Name ตำแหน่ง :  @hrPerson.Organizs.Positions.PositionName</small>
                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price"><small class="text-muted"> เบอร์โทร : </small>  @hrPerson.HRRecruiteTel </h2>
                                                    </div>
                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price"><small class="text-muted">Email : </small>  @hrPerson.HRRecruiteEmail </h2>
                                                    </div>
                                                    <hr>

                                                    <h4> </h4>

                                                    <div class="small text-muted">
                                                        <h4>
                                                            บัตรประจำตัวประชาชน : @hrPerson.HRRecruiteCardId
                                                        </h4>
                                                        <h4>

                                                            ชื่อเล่น : @hrPerson.HRRecruiteNickname
                                                        </h4>
                                                        <h4>
                                                            วันเกิด :  @hrPerson.HRRecruitBirth.ToString("dd/MM/yyyy")
                                                        </h4>
                                                        <h4>
                                                            Line Id : @hrPerson.HRRecruiteLineId
                                                        </h4>
                                                        <h4>
                                                            แหล่งที่มา : @hrPerson.HRRecruiteGroups.HRRecruiteGroupDetail
                                                        </h4>
                                                        <h4>
                                                            กรุ๊ปเลือด : @hrPerson.Bloods.BloodName
                                                        </h4>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="ibox-footer">
                                            <span class="pull-right">
                                                วันที่เพิ่มข้อมูล - <i class="fa fa-clock-o"></i> @hrPerson.HRRecruitDate.ToString("dd/MM/yyyy HH:mm:ss")
                                            </span>

                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="tab-4" class="tab-pane">
                    <div class="panel-body">
                        <strong>ประวัติการศึกษา : @hrPerson.TitleOfUsers.TitleOfUserName @hrPerson.HRRecruiteFName   @hrPerson.HRRecruiteLName </strong>

                        <div class="wrapper wrapper-content animated fadeInRight">

                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="ibox product-detail">
                                        <div class="ibox-content">

                                            <div class="row">
                                                <div class="col-md-9 col-md-offset-3">

                                                    <h2 class="font-bold m-b-xs">
                                                        @hrPerson.Universities.UniversiryName
                                                    </h2>

                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price">คณะ/หลักสูตร :  @hrPerson.Faculties.FacultyName </h2>
                                                    </div>
                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price">สาขา :   @hrPerson.Majors.MajorName </h2>
                                                    </div>
                                                    <hr>

                                                    <h4> </h4>

                                                    <div class="small text-muted">
                                                        <h4>
                                                            เกรดเฉลี่ย : @hrPerson.GPA
                                                        </h4>
                                                        <h4>

                                                            ปีที่สำเร็จการศึกษา : @hrPerson.YearCongrate
                                                        </h4>
                                                        <h4>
                                                            ระดับการศึกษาสูงสุด @hrPerson.TypeCongrates.TypeCongrateName
                                                        </h4>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="ibox-footer">
                                            <span class="pull-right">
                                                วันที่เพิ่มข้อมูล - <i class="fa fa-clock-o"></i> @hrPerson.HRRecruitDate.ToString("dd/MM/yyyy HH:mm:ss")
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="tab-5" class="tab-pane">
                    <div class="panel-body">
                        <strong>ประวัติการทำงาน : @hrPerson.TitleOfUsers.TitleOfUserName @hrPerson.HRRecruiteFName   @hrPerson.HRRecruiteLName </strong>
                        <div class="wrapper wrapper-content animated fadeInRight">

                            <div class="row">
                                <div class="col-lg-12">

                                    <div class="ibox product-detail">
                                        <div class="ibox-content">

                                            <div class="row">
                                                <div class="col-md-3">

                                                </div>
                                                <div class="col-md-9">

                                                    <h2 class="font-bold m-b-xs">

                                                        ตำแหน่งสุดท้าย : @hrPerson.LastPosition
                                                    </h2>

                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price">ประสบการณ์ทำงาน : @hrPerson.ExWorkYear </h2>
                                                    </div>
                                                    <div class="m-t-md">
                                                        <h2 class="product-main-price">สถานที่ทำงานล่าสุด : @hrPerson.LastWorkYear </h2>
                                                    </div>
                                                    <hr>

                                                    <h4> </h4>

                                                    <div class="small text-muted">
                                                        <h4>
                                                            ช่วงเงินเดือนสุดท้าย : @hrPerson.TypeOfSalaries.TypeOfSalaryName
                                                        </h4>
                                                        <h4>

                                                            สาเหตุที่ออกจากงาน : @hrPerson.TypeOfResigns.TypeOfResignName
                                                        </h4>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="ibox-footer">
                                            <span class="pull-right">
                                                วันที่เพิ่มข้อมูล - <i class="fa fa-clock-o"></i> @hrPerson.HRRecruitDate.ToString("dd/MM/yyyy HH:mm:ss")
                                            </span>

                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <form asp-action="Delete">
            <input type="hidden" asp-for="HRRecruiteID" />
            <input type="submit" value="Delete" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-info">Back to List</a>
        </form>
    </div>



}






















@section Scripts {







}





