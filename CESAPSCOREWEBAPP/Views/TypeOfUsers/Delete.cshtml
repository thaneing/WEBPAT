@model CESAPSCOREWEBAPP.Models.TypeOfUser

@{
    ViewData["Title"] = "ลบประเภทผู้ใช้";
    ViewData["Tab"] = "<ol class='breadcrumb'>"
                   + "<li>"
                   + "<a href='../../Home/Index' data-toggle='tooltip' data-placement='top' title='Home'>หน้าหลัก</a>"
                   + "</li>"
                   + "<li>"
                   + "<a href='/TypeOfUsers/Index' data-toggle='tooltip' data-placement='top' title='List'>รายการประเภทของ User</a>"
                   + "</li>"
                   + "<li class='active'>"
                   + "<b><a data-toggle='tooltip' data-placement='top' title='Add Type Of Users'>" + @ViewData["Title"] + "</a></b>"
                   + "</li>"
                   + "</ol>";
}

    <div class="col-sm-6 col-lg-6 col-md-6 col-lg-offset-3 ">
        <div class="ibox ">
            <div class="ibox-title">
                <h5>@ViewData["Title"]</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>

                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <div class="row">
                    <div class="col-lg-12">
                        <dt class="col-sm-3">
                            ประเภทผู้ใช้ :
                        </dt>
                        <dd class="col-sm-9">
                            @Html.DisplayFor(model => model.TypeOfUserName)
                        </dd>
                        </dl>
                    </div>
                </div>
            </div>
        </div>
        <div>

            <form asp-action="Delete">
                <input type="hidden" asp-for="TypeOfUserId" />
                <input type="submit" value="ลบ" class="btn btn-danger" data-toggle="tooltip" data-placement="top" title="Delete" />
                <a asp-action="Index" class="btn btn-info" data-toggle="tooltip" data-placement="top" title="Cancle">ยกเลิก</a>
            </form>
        </div>
    </div>



