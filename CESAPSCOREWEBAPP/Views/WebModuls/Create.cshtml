@model CESAPSCOREWEBAPP.Models.WebModul

@{
    ViewData["Title"] = "เพิ่มโมดูล";
    ViewData["Tab"] = "<ol class='breadcrumb'>"
                 + "<li>"
                 + "<a href='../../Home/Index' data-toggle='tooltip' data-placement='top' title='Home'>หน้าหลัก</a>"
                 + "</li>"
                 + "<li>"
                 + "<a href='/WebModuls/Index' data-toggle='tooltip' data-placement='top' title='List'>รายการโมดูล</a>"
                 + "</li>"
                 + "<li class='active'>"
                 + "<b><a data-toggle='tooltip' data-placement='top' title='Add Title Of Users'>" + @ViewData["Title"] + "</a></b>"
                 + "</li>"
                 + "</ol>";
}

<style>
    #new {
        height: 250px;
        /*overflow: auto;*/
    }

    .tableFixHead1 {
        overflow-y: auto;
        height: 180px;
    }

        .tableFixHead1 th {
            position: sticky;
            top: 0;
        }




    .tableFixHead {
        overflow-y: auto;
        height: 500px;
    }

        .tableFixHead th {
            position: sticky;
            top: 0;
        }

    /* Just common table stuff. Really. */
    table {
        border-collapse: collapse;
        width: 100%;
    }

    th, td {
        padding: 8px 16px;
    }

    th {
        background: #eee;
    }
</style>
<form asp-action="Create">
    <div class="col-sm-6 col-lg-6 col-md-6 col-lg-offset-3">
        <div class="ibox ">
            <div class="ibox-title">
                <h5>@ViewData["Title"]</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>

                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <div class="row">
                    <div class="col-lg-12">

                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group">
                            <label class="control-label">ชื่อโมดูล</label>
                            <input asp-for="WebModulName" class="form-control" />
                            <span asp-validation-for="WebModulName" class="text-danger"></span>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div>
            <input type="submit" value="บันทึก" class="btn btn-success" data-toggle="tooltip" data-placement="top" title="Save" />
            <a asp-action="Index" class="btn btn-info" data-toggle="tooltip" data-placement="top" title="Back to List">กลับรายการ</a>
        </div>
    </div>
</form>