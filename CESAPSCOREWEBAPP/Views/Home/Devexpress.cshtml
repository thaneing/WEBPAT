<script type="text/javascript" id="script">
    function customizeMenuActions(s, e) {
        // Get the "Save" action and hide it.
        var searchAction = e.GetById(DevExpress.Reporting.Viewer.ActionId.Search);
        if (searchAction)
            searchAction.visible = false;
    }
</script>


