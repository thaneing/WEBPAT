@model CESAPSCOREWEBAPP.Models.TitleOfUser

@{
    ViewData["Title"] = "ลบคำนำหน้านาม";
    ViewData["Tab"] = "<ol class='breadcrumb'>"
                   + "<li>"
                   + "<a href='../../Home/Index' data-toggle='tooltip' data-placement='top' title='Home'>หน้าหลัก</a>"
                   + "</li>"
                   + "<li>"
                   + "<a href='/TitleOfUsers/Index' data-toggle='tooltip' data-placement='top' title='List'>รายการคำนำหน้านาม</a>"
                   + "</li>"
                   + "<li class='active'>"
                   + "<b><a data-toggle='tooltip' data-placement='top' title='Add Title Of Users'>" + @ViewData["Title"] + "</a></b>"
                   + "</li>"
                   + "</ol>";
}

<div class="col-xs-12 col-sm-12  col-md-12 col-lg-6 col-lg-offset-3">
    <div class="ibox float-e-margins">
        <div class="ibox-title">

            <!-- TO DO List -->
            <div class="box box-primary">
                <div class="box-header">
                    <i class="ion ion-clipboard"></i>
                    <h3 class="box-title">@ViewData["Title"]</h3>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-lg-12">

                            <dl class="row">
                                <dt class="col-sm-3">
                                    คำนำหน้านาม :
                                </dt>
                                <dd class="col-sm-7">
                                    @Html.DisplayFor(model => model.TitleOfUserName)
                                </dd>
                            </dl>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <div>

        </div>
    </div>
    <form asp-action="Delete">
        <input type="hidden" asp-for="TitleOfUserId" />
        <input type="submit" value="ลบ" class="btn btn-danger" data-toggle="tooltip" data-placement="top" title="Delete" />
        <a asp-action="Index" class="btn btn-info" data-toggle="tooltip" data-placement="top" title="Cancle">ยกเลิก</a>
    </form>
</div>


