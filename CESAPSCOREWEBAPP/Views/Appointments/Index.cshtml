            <th>
                @Html.DisplayNameFor(model => model.AppUpdateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.appTelTypes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.appStatuses)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.appResults)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.appEtcs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.appSuccesses)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HRRecruiteID)
