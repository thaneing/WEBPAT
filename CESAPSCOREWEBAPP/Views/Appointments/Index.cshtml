@model IEnumerable<CESAPSCOREWEBAPP.Models.Appointment>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.HRRecruiteID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AppTelDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AppDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AppCreateBy)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AppCreateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AppUpdateBy)
