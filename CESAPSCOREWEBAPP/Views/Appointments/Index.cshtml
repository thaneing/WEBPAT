                @Html.DisplayFor(modelItem => item.AppUpdateBy)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AppUpdateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.appTelTypes.AppTelTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.appStatuses.AppStatusId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.appResults.AppResultId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.appEtcs.AppEtcId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.appSuccesses.AppSuccessId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AppId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AppId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AppId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
